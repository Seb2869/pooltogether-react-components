{
  "name": "@pooltogether/utilities",
  "version": "0.0.26",
  "description": "Shared utility functions across PoolTogether apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:pooltogether/pooltogether-utilities.git",
  "author": "PoolTogether Inc.",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsc-watch --onSuccess \"node ./dist/index.js\" ",
    "build": "tsc",
    "reinstall": "rm -rf node_modules/ && yarn install",
    "test": "jest",
    "testcov": "jest --coverage --coverageDirectory=coverage && open ./coverage/index.html",
    "watch": "yarn run test --watch"
  },
  "dependencies": {
    "@pooltogether/current-pool-data": "^3.4.3",
    "ethers": "^5.3.0",
    "lodash.clonedeep": "^4.5.0",
    "query-string": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coverageReporters": [
      "html"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).+(ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "yalcSig": "8d1939343d419be7c1941a9dc4a21484"
}
